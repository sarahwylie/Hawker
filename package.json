{
  "name": "hawker",
  "version": "1.0.0",
  "description": "A marketplace where users can buy or sell items",
  "main": "Develop/server/server.js",
  "scripts": {
    "start": "node Develop/server/server.js",
    "develop": "concurrently \"cd Develop/server && npm run watch\" \"cd Develop/client && npm start\"",
    "build": "cd Develop/client && npm run build",
    "test": "cd Develop/client && npm run test",
    "install": "cd Develop/client && npm i && cd ../server && npm i",
    "eject": "cd Develop/client && npm run eject",
    "eslint": "cd Develop/client && npm run eslint && cd ../server && npm run eslint",
    "seed": "cd Develop/server && npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicnolen/hawker.git"
  },
  "keywords": [
    "marketplace",
    "buy",
    "sell",
    "items"
  ],
  "author": "Nicholas Nolen, Craig Jensen, Jide Ogunbanjo, Nigel Loch, Samantha Chavez, Sarah Wylie",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicnolen/hawker/issues"
  },
  "homepage": "https://texas-hawker.herokuapp.com/",
  "devDependencies": {
    "concurrently": "^7.1.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.29.4",
    "postcss-loader": "~3.0.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@testing-library/react": "^13.0.1",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jwt-decode": "^3.1.2",
    "mongoose": "^6.2.10",
    "nodemon": "^2.0.15",
    "react-bootstrap": "^2.2.3",
    "react-router-dom": "^6.3.0"
  }
}
